{
  "name": "gpx2czml",
  "version": "1.0.6",
  "description": "gpx to czml converter module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "HanseungYoo (trustyoo86@gmail.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/trustyoo86/gpx2czml.git"
  },
  "keywords": [
    "gpx",
    "cesiumjs",
    "3d",
    "javascript",
    "es6",
    "czml"
  ],
  "scripts": {
