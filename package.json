    "log:major": "changelog -M",
    "log:file": "changelog -f"
  },
  "dependencies": {
    "Promise": "^1.0.5",
    "all": "^0.0.0",
    "jquery": ">=3.4.1",
    "lodash": "^4.17.15",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "path": "^0.12.7",
    "should": "^13.2.3",
    "sinon": "^7.4.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "eslint": "^8.40.0",
